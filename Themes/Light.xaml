<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Title colors -->
    <Color x:Key="TitleYellow">#FFFFF2AB</Color>
    <Color x:Key="TitleGreen">#FFCBF1C4</Color>
    <Color x:Key="TitlePink">#FFFFCCE5</Color>
    <Color x:Key="TitlePurple">#FFE7CFFF</Color>
    <Color x:Key="TitleBlue">#FFCDE9FF</Color>
    <Color x:Key="TitleGray">#FFE1DFDD</Color>
    <Color x:Key="TitleCarbon">#FF494745</Color>

    <!-- Content colors -->
    <Color x:Key="ContentYellow">#FFFFF7D1</Color>
    <Color x:Key="ContentGreen">#FFE4F9E0</Color>
    <Color x:Key="ContentPink">#FFFFE4F1</Color>
    <Color x:Key="ContentPurple">#FFF2E6FF</Color>
    <Color x:Key="ContentBlue">#FFE2F1FF</Color>
    <Color x:Key="ContentGray">#FFF3F2F1</Color>
    <Color x:Key="ContentCarbon">#FF696969</Color>

    <!-- Picker colors -->
    <Color x:Key="PickerYellow">#FFFFE66E</Color>
    <Color x:Key="PickerGreen">#FFA1EF9B</Color>
    <Color x:Key="PickerPink">#FFFFAFDF</Color>
    <Color x:Key="PickerPurple">#FFD7AFFF</Color>
    <Color x:Key="PickerBlue">#FF9EDFFF</Color>
    <Color x:Key="PickerGray">#FFE0E0E0</Color>
    <Color x:Key="PickerCarbon">#FF767676</Color>
</ResourceDictionary>